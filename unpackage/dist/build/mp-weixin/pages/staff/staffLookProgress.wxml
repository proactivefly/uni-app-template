<view class="wrapper"><view class="user_name">{{clientOrder.memberName}}</view><view class="switch"><scroll-view scroll-x="{{true}}" scroll-with-animation="{{true}}" enable-back-to-top="{{true}}" scroll-into-view="{{domIndex}}"><view class="list" style="{{'width:'+(parentWidth*130+'px')+';'}}"><block wx:for="{{clientOrder.orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[idx==index?'active':'','item']}}" id="{{'dom'+index}}" data-event-opts="{{[['tap',[['changeSlide',['$0',index],[[['clientOrder.orderList','',index]]]]]]]}}" bindtap="__e"><view class="want">意向车型</view><view class="modal">{{item.carTypeName}}</view></view></block></view></scroll-view></view><view class="cu-timeline"><block wx:for="{{progressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',currentStep>=item.step?'text-blue':'no_pass',currentStep==item.step?'active':'']}}"><view class="look_car time_item"><view class="title"><text>{{item.name}}</text></view><block wx:if="{{item.step<6}}"><block><block wx:if="{{currentStep>=item.step}}"><view class="select_date">{{item.nodeDate}}</view></block></block></block><block wx:else><block><block wx:if="{{currentStep>=item.step}}"><view class="select_date">{{item.nodeDate}}</view></block><block wx:if="{{currentStep>=item.step}}"><view class="upload_w"><view class="upload"><block wx:for="{{affixList}}" wx:for-item="lett" wx:for-index="idx" wx:key="idx"><view class="item"><view data-event-opts="{{[['tap',[['lookAffix',['$0','$1'],[[['affixList','',idx]],'affixList']]]]]}}" class="inner_w" bindtap="__e"><image class="pic _img" src="{{lett}}"></image></view></view></block></view></view></block></block></block></view></view></block></view><view class="cu-timeline"><view class="cu-item text-blue"><view class="look_car time_item"><view class="title flex-row-start"><text>员工上传推荐购车凭证</text><block wx:if="{{currentOrderInfo.orderState==-1}}"><text class="status">{{currentOrderInfo.orderStateName}}</text></block></view><view class="upload_w"><view class="upload"><block wx:for="{{staffAffixList}}" wx:for-item="litt" wx:for-index="idx" wx:key="idx"><view class="item"><block wx:if="{{!hasStaffAffix}}"><image class="delete _img" src="{{$root.m0}}" data-event-opts="{{[['tap',[['deleteFile',['$0'],[[['staffAffixList','',idx]]]]]]]}}" bindtap="__e"></image></block><view data-event-opts="{{[['tap',[['staffLookPic',['$0','$1'],[[['staffAffixList','',idx,'openUrl']],'staffAffixList']]]]]}}" class="inner_w" bindtap="__e"><image class="pic" src="{{litt.openUrl}}"></image></view></view></block><block wx:if="{{staffAffixList.length<3&&!hasStaffAffix&&currentOrderInfo.orderState!=-1}}"><view data-event-opts="{{[['tap',[['takePicture',['$event']]]]]}}" class="inner item" bindtap="__e"><image src="../../static/pic.png"></image></view></block></view><block wx:if="{{!hasStaffAffix&&currentOrderInfo.orderState!=-1}}"><view data-event-opts="{{[['tap',[['uploadAffix',['$event']]]]]}}" class="make_sure" bindtap="__e"><button class="{{['cu-btn','block','text-qxblue',affixBtnDisabled?'disabled':'']}}" disabled="{{affixBtnDisabled}}">确认上传</button></view></block></view></view></view><view class="{{['cu-item',currentOrderInfo.orderState>=15?'text-blue':'no_pass']}}"><view class="look_car time_item"><view class="title">订单评分</view><block wx:if="{{rateScore>0}}"><view class="sd"><view class="rate_w"><view class="rate_item"><evaluate class="rate" vue-id="6458acf2-1" size="{{36}}" margin="0 15" value="{{rateScore}}" text="{{rateText}}" disabled="{{true}}" textColor="#0096EA" star_fill="/static/blue_start.png" bind:__l="__l"></evaluate></view></view></view></block><block wx:if="{{canRate&&currentOrderInfo.orderState!=-1}}"><view class="upload_w"><view data-event-opts="{{[['tap',[['goRate',['$event']]]]]}}" class="make_sure" bindtap="__e"><button class="{{['cu-btn','block','text-qxblue']}}">去评价</button></view></view></block></view></view></view><view class="{{['cu-modal',lookImg?'show':'']}}" catchtouchmove="return"><view class="cu-dialog"><view class="bg-img"><image class="dialogImg" src="{{currentImgUrl}}" mode="widthFix"></image></view><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action margin-0 flex-sub" bindtap="__e">关闭</view></view></view></view><view class="{{['cu-modal rate_dialog',rateDialog?'show':'']}}" catchtouchmove="return"><view class="cu-dialog padding"><view class="content text-left"><view class="title">订单评分</view><view class="tip text-blue">需完成评价再确认上传</view><view class="rate_w"><view class="rate_item"><evaluate class="rate" vue-id="6458acf2-2" value="{{rateScore}}" size="{{36}}" star_fill="/static/blue_start.png" textColor="#0096EA" margin="0 15" text="{{rateText}}" data-event-opts="{{[['^change',[['startChange']]]]}}" bind:change="__e" bind:__l="__l"></evaluate></view></view></view><view data-event-opts="{{[['tap',[['makeScore',['$event']]]]]}}" class="action margin-0 flex-sub" bindtap="__e"><button class="cu-btn make_sure">确认评分</button></view></view></view></view>